r\n  background: url(\"../assets/background.jpg\");\r\n  background-position: center;\r\n  background-size: 100%;\r\n  background-repeat: no-repeat;\r\n  animation-name: backgroundAnimation;\r\n  animation-duration: 15s;\r\n  animation-timing-function: cubic-bezier(0.5, 0.5, 0.5, 0.5);\r\n  animation-iteration-count: infinite;\r\n  animation-direction: alternate;\r\n}\r\n\r\n@keyframes backgroundAnimation {\r\n  0% {\r\n    background-size: 100%;\r\n  }\r\n  100% {\r\n    background-size: 140%;\r\n  }\r\n}\r\n\r\n.search {\r\n  position: fixed;\r\n  background-color: rgba(255, 255, 255, 0.4);\r\n  border-radius: 15px;\r\n  backdrop-filter: blur(5px);\r\n  width: 300px;\r\n  text-align: center;\r\n  padding: 15px 5px;\r\n  transform: translateY(-100%);\r\n  left: calc(50% - 300px / 2);\r\n  transition: transform 0.2s ease-in;\r\n\r\n  &__input {\r\n    border: none;\r\n    border-bottom: 1px solid rgba($color: #000000, $alpha: 0.3);\r\n    background-color: transparent;\r\n    width: 90%;\r\n    font-size: 1.4rem;\r\n    font-weight: 500;\r\n    color: rgba($color: #000000, $alpha: 0.8);\r\n  }\r\n  &__input:focus {\r\n    outline: none;\r\n  }\r\n  &__input::placeholder {\r\n    color: rgba($color: #000000, $alpha: 0.4);\r\n  }\r\n}\r\n.show-search {\r\n  transform: translateY(3px);\r\n}\r\n\r\n$width-window: 500px;\r\n.window {\r\n  position: fixed;\r\n  width: $width-window;\r\n  height: 400px;\r\n  background-color: rgba(255, 255, 255, 0.4);\r\n  top: calc(50% - $width-window/2);\r\n  left: calc(50% - $width-window/2);\r\n  border-radius: 30px;\r\n  backdrop-filter: blur(5px);\r\n}\r\n.window__header {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 1.8rem;\r\n  font-weight: 700;\r\n  padding-top: 30px;\r\n  color: ghostwhite;\r\n  text-shadow: 0 0 5px rgba(0, 0, 0, 0.4);\r\n  opacity: 0;\r\n  transition: opacity 0.3s ease-in;\r\n}\r\n.window__content {\r\n  font-size: 5rem;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-weight: 500;\r\n  text-align: center;\r\n  padding-top: 55px;\r\n  color: ghostwhite;\r\n  text-shadow: 0 0 5px rgba(0, 0, 0, 0.4);\r\n  opacity: 0;\r\n  transition: opacity 0.3s ease-in;\r\n}\r\n.window__footer {\r\n  font-size: 1.5rem;\r\n  display: flex;\r\n  width: 100%;\r\n  height: 10%;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-weight: 300;\r\n  color: ghostwhite;\r\n  text-shadow: 0 0 5px rgba(0, 0, 0, 0.4);\r\n  opacity: 0;\r\n  transition: opacity 0.3s ease-in;\r\n}\r\n.window-show-content {\r\n  opacity: 1;\r\n}\r\n\r\n#header-city::after {\r\n  content: \"|\";\r\n  padding: 0 8px;\r\n}\r\n#temperature {\r\n  position: relative;\r\n}\r\n#weather_icon {\r\n  position: absolute;\r\n  left: 55px;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"43759317c6c63bdb4b23f2c73e901158","cacheData":{"env":{}}}